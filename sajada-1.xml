            <Root name="بطح" transliteration="bTH"/>
            <Deverbal name="إِنْبِتَاح" transliteration="IinobitaAH" transname="إنبتاح"/>
            <Participle name="مُنْبَتِح" transliteration="munobatiH" transname="منبتح"/>
        </MEMBER>
        <!-- In so fare as the action implie the body in the action-->
        <MEMBER name="أَنَاخَ" transliteration="OanaAxa" transname="أناخ">
            <Root name="ناخ" transliteration="nAx"/>
            <Deverbal name="إِنَاخَة" transliteration="IinaAxap" transname="إناخة"/>
            <Participle name="نَائِخ" transliteration="naA}ix" transname="نائخ"/>
        </MEMBER>
