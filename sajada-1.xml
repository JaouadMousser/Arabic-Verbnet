<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE AVNCLASS SYSTEM "vn_class-3.dtd">
<AVNCLASS ID="sajada-1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="vn_schema-3.xsd">
    <MEMBERS>
        <MEMBER name="سَجَدَ" transliteration="sajada" transname="سجد">
            <Root name="سجد" transliteration="sjd"/>
            <Deverbal name="سُجُود" transliteration="sujuwd" transname="سجود"/>
            <Participle name="سَاجِد" transliteration="saAjid" transname="ساجد"/>
        </MEMBER>
        <MEMBER name="إِنْحَنَى" transliteration="IinoHanaY" transname="إنحنى">
