                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
        <FRAME>
            <DESCRIPTION primary="V NP PP.recipient" secondary="PPRecipient-PP"/>
            <EXAMPLES>
                <EXAMPLE sentence="سَجَد المُبايِعِونَ لِلمَلِك." transliteration="sajad AlmubAyiEiwna lilmalik."/>
