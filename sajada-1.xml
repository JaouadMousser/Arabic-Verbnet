            <Participle name="مُوَسِّع" transliteration="muwas~iE" transname="موسع"/>
        </MEMBER>
        <MEMBER name="بَنْدَقَ" transliteration="banodaqa" transname="بندق">
            <Root name="بندق" transliteration="bndq"/>
            <Deverbal name="بَنْدَقَة" transliteration="banodaqap" transname="بندقة"/>
            <Participle name="مُبَنْدِق" transliteration="mubanodiq" transname="مبندق"/>
        </MEMBER>
