    <THEMROLES>
        <!--Patient1 is interpreted as Figure, Patient2 as Ground-->
        <!--Each verb specifies an Instrument-->
        <THEMROLE type="Agent">
            <SELRESTRS logic="or">
                <SELRESTR Value="+" type="animate"/>
                <SELRESTR Value="+" type="machine"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Patient">
            <SELRESTRS>
                <SELRESTR Value="+" type="solid"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Patient1">
            <SELRESTRS>
                <SELRESTR Value="+" type="solid"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Patient2">
            <SELRESTRS>
                <SELRESTR Value="+" type="solid"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Instrument">
            <SELRESTRS>
                <SELRESTR Value="+" type="concrete"/>
                <SELRESTR Value="-" type="animate"/>
            </SELRESTRS>
        </THEMROLE>
        <!--Instrument is realized in the verb-->
    </THEMROLES>
    <FRAMES>
        <FRAME>
            <DESCRIPTION primary="V NP NP" secondary="Basic Transitive"/>
            <EXAMPLES>
                <EXAMPLE sentence="أَلْصَقَ سَلِيم الصُورَةَ." transliteration="OaloSaqa saliym AlSuwrapa."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <NP value="Patient">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
                <PRED value="attached">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Patient"/>
                        <ARG type="VerbSpecific" value="Instrument"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
        <FRAME>
            <DESCRIPTION primary="V NP NP PP.patient2" secondary="NP-PPlocative-PP"/>
            <EXAMPLES>
                <EXAMPLE sentence="أَلْصَقَ سَلِيمُ الصُورَة بِالحَائِط." transliteration="OaloSaqa saliymu AlSuwrap biAlHaA}iT."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <NP value="Patient1">
                    <SYNRESTRS/>
                </NP>
                <PREP value="بِ عَلَى فَوْقَ">
                    <SELRESTRS/>
                </PREP>
                <NP value="Patient2">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
                <PRED value="together">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="Constant" value="physical"/>
                        <ARG type="ThemRole" value="Patient1"/>
                        <ARG type="ThemRole" value="Patient2"/>
                    </ARGS>
                </PRED>
                <PRED value="attached">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Patient1"/>
                        <ARG type="VerbSpecific" value="Instrument"/>
                    </ARGS>
                </PRED>
                <PRED value="attached">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Patient2"/>
                        <ARG type="VerbSpecific" value="Instrument"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
    </FRAMES>
