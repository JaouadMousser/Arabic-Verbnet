    <THEMROLES>
        <THEMROLE type="Agent">
            <SELRESTRS logic="or">
                <SELRESTR Value="+" type="animate"/>
                <SELRESTR Value="+" type="organization"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Theme">
            <SELRESTRS>
                <SELRESTR Value="+" type="animate"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Source">
            <SELRESTRS>
                <SELRESTR Value="+" type="location"/>
            </SELRESTRS>
        </THEMROLE>
        <THEMROLE type="Destination">
            <SELRESTRS>
                <SELRESTR Value="+" type="location"/>
                <SELRESTR Value="-" type="region"/>
            </SELRESTRS>
        </THEMROLE>
    </THEMROLES>
    <FRAMES>
        <FRAME>
            <DESCRIPTION primary="V NP NP" secondary="Basic Transitive"/>
            <EXAMPLES>
                <EXAMPLE sentence="نَفَى العَسْكَرُ كُلَّ المُعَارِضِين." transliteration="nafaY AlEasokaru kul~a AlmuEaAriDiyn."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="?Source"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="?Destination"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
        <FRAME>
            <DESCRIPTION primary="V NP NP PP.source" secondary="NP-PPSource-PP"/>
            <EXAMPLES>
                <EXAMPLE sentence="نَفَى العَسْكَر المُعَارِضِينَ مِنَ الَبَلَدِ." transliteration="nafaY AlEasokar AlmuEaAriDiyna mina Alabaladi."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
                <PREP>
                    <SELRESTRS>
                        <SELRESTR Value="+" type="src"/>
                    </SELRESTRS>
                </PREP>
                <NP value="Source">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Source"/>
                    </ARGS>
                </PRED>
                <PRED bool="!" value="location">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Source"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
        <FRAME>
            <DESCRIPTION primary="V NP NP PP.destination" secondary="NP-PPDestination-PP"/>
            <!--"remove"? *remove the book to the table-->
            <EXAMPLES>
                <EXAMPLE sentence="نَفَتْ سُلُطَات الحِمَايَة المَلِكَ إِلَى مَدَغَشْقَر." transliteration="nafato suluTaAt AlHimaAyap Almalika IilaY madaga$oqar."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
                <PREP value="إِلَى">
                    <SELRESTRS/>
                </PREP>
                <NP value="Destination">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="?Source"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Destination"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
    </FRAMES>
    <SUBCLASSES/>
    <SIBLINGCLASSES/>
</AVNCLASS>
