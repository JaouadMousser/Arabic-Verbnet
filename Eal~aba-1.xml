    <FRAMES>
        <FRAME>
            <DESCRIPTION primary="V NP NP.theme" secondary="Basic TransitiveTheme Object"/>
            <!-- Location specified by verb-->
            <EXAMPLES>
                <EXAMPLE sentence="عَلَّبَ العُمَّالُ السَّمَكَ." transliteration="Eal~aba AlEum~aAlu Als~amaka."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="motion">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED bool="!" value="location">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Location"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Location"/>
                    </ARGS>
                </PRED>
                <PRED value="cause">
                    <ARGS>
                        <ARG type="ThemRole" value="Agent"/>
                        <ARG type="Event" value="E"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
        <FRAME>
            <DESCRIPTION primary="V NP NP PP.location" secondary="NP-PPLocation-PP"/>
            <!-- Location must be a hyponym of verb's zero-related nominal, or specify additional information-->
            <!-- Acceptability is marginal-->
            <EXAMPLES>
                <EXAMPLE sentence="عَلَبَّ العُمَّالُ السَّمَكَ فِي صَفَائِح." transliteration="Ealab~a AlEum~aAlu Als~amaka fiy SafaA}iH."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Agent">
                    <SYNRESTRS/>
                </NP>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
                <PREP value="فِي">
                    <SELRESTRS/>
                </PREP>
                <NP value="Location">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="motion">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED bool="!" value="Prep">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Location"/>
                    </ARGS>
                </PRED>
                <PRED value="Prep">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
