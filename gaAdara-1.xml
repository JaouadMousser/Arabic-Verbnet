                        <ARG type="ThemRole" value="Source"/>
                    </ARGS>
                </PRED>
                <PRED value="direction">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="Constant" value="from"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Source"/>
                    </ARGS>
                </PRED>
            </SEMANTICS>
        </FRAME>
