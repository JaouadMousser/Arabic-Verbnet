        </THEMROLE>
        <THEMROLE type="Source">
            <SELRESTRS>
                <SELRESTR Value="+" type="location"/>
                <SELRESTR Value="-" type="region"/>
            </SELRESTRS>
        </THEMROLE>
    </THEMROLES>
    <FRAMES>
        <FRAME>
            <DESCRIPTION primary="Transitive" secondary="basically, locative preposition drop of &quot;from&quot;"/>
            <EXAMPLES>
                <EXAMPLE sentence="غَادَرَ المُفَتِّشُونَ المَكَانَ." transliteration="gaAdara Almufat~i$uwna AlmakaAna."/>
            </EXAMPLES>
            <SYNTAX>
                <VERB/>
                <NP value="Theme">
                    <SYNRESTRS/>
                </NP>
                <NP value="Source">
                    <SYNRESTRS/>
                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="motion">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
                    <ARGS>
                        <ARG type="Event" value="start(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                        <ARG type="ThemRole" value="Source"/>
                    </ARGS>
                </PRED>
                <PRED bool="!" value="location">
                    <ARGS>
                        <ARG type="Event" value="end(E)"/>
