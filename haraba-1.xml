                </NP>
            </SYNTAX>
            <SEMANTICS>
                <PRED value="motion">
                    <ARGS>
                        <ARG type="Event" value="during(E)"/>
                        <ARG type="ThemRole" value="Theme"/>
                    </ARGS>
                </PRED>
                <PRED value="location">
